rimaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["macOS"],"tokens":[{"text":"static","kind":"keyword"},{"text":" ","kind":"text"},{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"rpn","kind":"identifier"},{"text":"(","kind":"text"},{"text":"parameter","kind":"externalParam"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"UInt7Pair","identifier":"doc:\/\/MIDIKitCore\/documentation\/MIDIKitCore\/UInt7Pair","preciseIdentifier":"s:11MIDIKitCore9UInt7PairV"},{"text":", ","kind":"text"},{"text":"data","kind":"externalParam"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","identifier":"doc:\/\/MIDIKitCore\/documentation\/MIDIKitCore\/UInt14","preciseIdentifier":"s:11MIDIKitCore6UInt14V","text":"UInt14"},{"kind":"text","text":", "},{"text":"change","kind":"externalParam"},{"kind":"text","text":": "},{"identifier":"doc:\/\/MIDIKitCore\/documentation\/MIDIKitCore\/MIDI2ParameterNumberChange","preciseIdentifier":"s:11MIDIKitCore26MIDI2ParameterNumberChangeO","text":"MIDI2ParameterNumberChange","kind":"typeIdentifier"},{"text":" = .absolute, ","kind":"text"},{"kind":"externalParam","text":"channel"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:11MIDIKitCore5UInt4V","text":"UInt4","identifier":"doc:\/\/MIDIKitCore\/documentation\/MIDIKitCore\/UInt4","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"kind":"externalParam","text":"group"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"UInt4","preciseIdentifier":"s:11MIDIKitCore5UInt4V","identifier":"doc:\/\/MIDIKitCore\/documentation\/MIDIKitCore\/UInt4"},{"text":" = 0x0) -> ","kind":"text"},{"text":"MIDIEvent","kind":"typeIdentifier","identifier":"doc:\/\/MIDIKitCore\/documentation\/MIDIKitCore\/MIDIEvent","preciseIdentifier":"s:11MIDIKitCore9MIDIEventO"}],"languages":["swift"]}]},{"kind":"parameters","parameters":[{"name":"parameter","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Registered Parameter Number (Registered Controller)."}]}]},{"content":[{"inlineContent":[{"type":"text","text":"MIDI 1.0 14-bit data value."}],"type":"paragraph"}],"name":"data"},{"content":[{"inlineContent":[{"type":"text","text":"Determines whether the value is absolute or a relative change. MIDI 1.0 is"},{"type":"text","text":" "},{"text":"always absolute.","type":"text"}],"type":"paragraph"}],"name":"change"},{"name":"channel","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Channel Number ("},{"code":"0x0 ... 0xF","type":"codeVoice"},{"type":"text","text":")"}]}]},{"name":"group","content":[{"inlineContent":[{"type":"text","text":"UMP Group ("},{"type":"codeVoice","code":"0x0 ... 0xF"},{"text":")","type":"text"}],"type":"paragraph"}]}]},{"kind":"content","content":[{"type":"heading","level":2,"text":"Discussion","anchor":"discussion"},{"name":"MIDI 1.0 Spec","style":"note","type":"aside","content":[{"inlineContent":[{"text":"","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"In order to set or change the value of a Registered Parameter (RPN), the following occurs:","type":"text"}]},{"items":[{"content":[{"inlineContent":[{"text":"Two Control Change messages are sent using CC 101 (0x65) and 100 (0x64) to select the","type":"text"},{"text":" ","type":"text"},{"text":"desired Registered Parameter Number.","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"When setting the Registered Parameter to a specific value, CC messages are sent to the"},{"text":" ","type":"text"},{"text":"Data Entry MSB controller (CC 6). If the selected Registered Parameter requires the LSB to","type":"text"},{"type":"text","text":" "},{"type":"text","text":"be set, another CC message is sent to the Data Entry LSB controller (CC 38)."}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"To make a relative adjustment to the selected Registered Parameterâ€™s current value, use"},{"type":"text","text":" "},{"type":"text","text":"the Data Increment or Data Decrement controllers (CCs 96 & 97)."}]}]}],"type":"orderedList"},{"inlineContent":[{"text":"Currently undefined RPN parameter numbers are all RESERVED for future MMA Definition.","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"For custom Parameter Number use, see NRPN (Non-Registered Parameter Numbers)."}]}]},{"name":"MIDI 2.0 Spec","style":"note","type":"aside","content":[{"inlineContent":[{"text":"","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"In the MIDI 2.0 Protocol, Registered Controllers (RPN) and Assignable Controllers (NRPN)","type":"text"},{"type":"text","text":" "},{"type":"text","text":"use a single, unified message, making them much easier to use."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"As a result, CC 6, 38, 98, 99, 100, and 101 are not to be used in standalone CC messages,","type":"text"},{"text":" ","type":"text"},{"text":"as the new MIDI 2.0 RPN\/NRPN UMP messages replace them.","type":"text"}]},{"inlineContent":[{"type":"text","text":"Registered Controllers (RPNs) have specific functions defined by MMA\/AMEI specifications."},{"type":"text","text":" "},{"text":"Registered Controllers map and translate directly to MIDI 1.0 Registered Parameter Numbers","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"(RPN, see Appendix D.2.3) and use the same definitions as MMA\/AMEI approved RPN messages."},{"text":" ","type":"text"},{"text":"Registered Controllers are organized in 128 Banks (corresponds to RPN MSB), with 128","type":"text"},{"text":" ","type":"text"},{"text":"controllers per Bank (corresponds to RPN LSB).","type":"text"}],"type":"paragraph"}]},{"style":"note","name":"Note","content":[{"inlineContent":[{"text":"See Recommended Practise","type":"text"},{"text":" ","type":"text"},{"isActive":true,"identifier":"https:\/\/www.midi.org\/specifications\/midi1-specifications\/midi-