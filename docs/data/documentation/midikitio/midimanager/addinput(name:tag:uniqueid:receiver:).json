aragraph"},{"inlineContent":[{"type":"text","text":"Do not generate the ID number yourself - it is always system-generated and then we should"},{"type":"text","text":" "},{"type":"text","text":"store and persist it. "},{"isActive":true,"type":"reference","identifier":"doc:\/\/MIDIKitIO\/documentation\/MIDIKitIO\/MIDIIdentifierPersistence"},{"type":"text","text":" offers mechanisms to simplify this."}],"type":"paragraph"}]},{"style":"note","name":"Throws","type":"aside","content":[{"inlineContent":[{"text":"","type":"text"},{"isActive":true,"identifier":"doc:\/\/MIDIKitIO\/documentation\/MIDIKitIO\/MIDIIOError","type":"reference"}],"type":"paragraph"}]}],"kind":"content"}],"identifier":{"url":"doc:\/\/MIDIKitIO\/documentation\/MIDIKitIO\/MIDIManager\/addInput(name:tag:uniqueID:receiver:)","interfaceLanguage":"swift"},"sections":[],"abstract":[{"text":"Creates a new managed virtual input in the system and adds it to the ","type":"text"},{"type":"reference","identifier":"doc:\/\/MIDIKitIO\/documentation\/MIDIKitIO\/MIDIManager\/managedInputs","isActive":true},{"type":"text","text":" "},{"type":"text","text":"dictionary of the "},{"type":"reference","identifier":"doc:\/\/MIDIKitIO\/documentation\/MIDIKitIO\/MIDIManager","isActive":true},{"type":"text","text":"."}],"kind":"symbol","hierarchy":{"paths":[["doc:\/\/MIDIKitIO\/documentation\/MIDIKitIO","doc:\/\/MIDIKitIO\/documentation\/MIDIKitIO\/MIDIManager"]]},"seeAlsoSections":[{"identifiers":["doc:\/\/MIDIKitIO\/documentation\/MIDIKitIO\/MIDIManager\/addOutput(name:tag:uniqueID:)"],"title":"Create Ports","generated":true,"anchor":"Create-Ports"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/midikitio\/midimanager\/addinput(name:tag:uniqueid:receiver:)"]}],"metadata":{"roleHeading":"Instance Method","extendedModule":"MIDIKitIO","modules":[{"name":"MIDIKitIO"}],"symbolKind":"method","title":"addInput(name:tag:uniqueID:receiver:)","role":"symbol","fragments":[{"kind":"keyword","text":"fu